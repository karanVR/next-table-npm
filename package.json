{
  "name": "next-table",
  "version": "0.0.1",
  "description": "A Super dynamic Table / Dashboard components that have a beautiful UI, built-in filters and pagination, comes with an optional side panel, that shows selected row data!",
  "main": "./dist/next-table.umd.js",
  "module": "./dist/next-table.es.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf node_modules/react node_modules/react-dom",
    "test": "npm run test",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karanVR/next-table-npm"
  },
  "keywords": [
    "table",
    "nextjs"
  ],
  "author": "Karan veer Raghuvanshi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karanVR/next-table-npm/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/karanVR/next-table-npm#readme",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "lucide-react": "^0.427.0",
    "prettier": "3.3.3",
    "react-icons": "^5.3.0",
    "shadcn-ui": "^0.8.0",
    "tailwindcss": "^3.4.9",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.2",
    "vite-plugin-linter": "^3.0.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@radix-ui/react-icons": "^1.3.0",
